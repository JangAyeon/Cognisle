{
  "semi": false,
  "singleQuote": false,
  "tabWidth": 2,
  "useTabs": false,
  "importOrder": [
    "^@/components/(.*)$",
    "^@/constants/(.*)$",
    "^@/hooks/(.*)$",
    "^@/interfaces/(.*)$",
    "^@/lib/(.*)$",
    "^@/mocks/(.*)$",
    "^@/pages/(.*)$",
    "^@/public/(.*)$",
    "^@/redux/(.*)$",
    "^@/styles/(.*)$",
    "^@/utils/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
